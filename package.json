{
  "name": "aero-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://hgzdev.github.io/aero-app",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
